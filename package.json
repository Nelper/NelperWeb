{
  "name": "nelper-web",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.js",
  "repository": "https://github.com/Nelper/NelperWeb",
  "license": "proprietary",
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run start-server",
    "start-server": "node ./build/server/server.js",
    "build": "webpack --config webpack/webpack.config.js",
    "watch": "babel-node ./src/server/devServer.js",
    "watch-client": "babel-node ./src/server/devServer.js --client",
    "watch-server": "babel-node ./src/server/devServer.js --server",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "clean": "rm -rf ./build",
    "test": "jest",
    "lint": "eslint ./src"
  },
  "engines": {
    "node": "^5.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/scripts/jestPreprocessor.js",
    "setupEnvScriptFile": "<rootDir>/scripts/jestEnvironnement.js",
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/graphql",
      "<rootDir>/node_modules/graphql-relay",
      "<rootDir>/node_modules/parse"
    ]
  },
  "dependencies": {
    "alt": "^0.18.2",
    "alt-utils": "^1.0.0",
